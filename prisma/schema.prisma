
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


enum Rol{
  USER
  ADMIN
}

//modelo basico para los usuarios

model User {
  id         String   @id @default(cuid())
  name       String   @unique
  email      String   @unique
  password   String
  rol        Rol      @default(USER)
  createdAt  DateTime @default(now())
  
  @@map("users")
}